{
  "name": "fintrack-api",
  "version": "0.1.0",
  "description": "Fintrack REST API.",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "start": "node dist/server.js",
    "predev": "npm run build",
    "dev": "concurrently \"npx tsc -w\" \"nodemon --ext js,yml dist/server.js\"",
    "prod": "dotenv -e .env.production.local -- concurrently \"npx tsc -w\" \"nodemon dist/server.js\"",
    "prisma": "prisma",
    "prisma:migrate:dev": "npm run prisma -- migrate dev",
    "prisma:migrate:deploy": "npm run prisma -- migrate deploy",
    "prisma:generate": "npm run prisma -- generate",
    "prisma:studio": "npm run prisma -- studio",
    "prisma:seed": "tsx src/prisma/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BODMAT/FinTrack.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "express",
    "prisma",
    "finance",
    "dashboard"
  ],
  "author": "Makar Dzhehur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BODMAT/FinTrack/issues"
  },
  "homepage": "https://github.com/BODMAT/FinTrack#readme",
  "dependencies": {
    "@fintrack/types": "*",
    "@prisma/client": "^6.14.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.18.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "4.19.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "concurrently": "^9.2.1",
    "dotenv-cli": "^10.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.14.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
