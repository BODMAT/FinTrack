	},
	"homepage": "https://github.com/BODMAT/FinTrack#readme",
	"dependencies": {
		"@prisma/client": "^6.14.0",
		"cors": "^2.8.5",
		"express": "^5.1.0",
		"zod": "^4.1.3"
	},
	"devDependencies": {
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.3",
		"@types/node": "^24.3.0",
		"concurrently": "^9.2.1",
		"nodemon": "^3.1.10",
